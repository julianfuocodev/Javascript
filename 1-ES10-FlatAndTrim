// flat
const array = [1,1,2,3,4, [1,3,5,6,[1,2,4]]];
console.log(array.flat(3)); //aplana todas las dimensiones

// flatmap
const array2 = [1,2,3,4,5];
console.log(array2.flatMap(v => [v, v *2]));


const hello = "       Hello World!   ";
console.log(hello.trimStart()); //saca espacios al inicio
console.log(hello.trimEnd()); //saca espacios al final
